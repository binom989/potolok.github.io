};return"all"===r?i.filters.date="":i.filters.date[r]="now",a&&(i.slice=a),o&&(i.getparts=!0),i}function t_feed_displayButtons(e,t,o){var a=$("#rec"+e);null!==a.find(".t-feed__buttons-wrapper")&&(a.find(".t-feed__buttons-wrapper").removeClass("t-feed__buttons-wrapper_hidden"),void 0===t?a.find(".js-feed-btn-show-more").hide():a.find(".js-feed-btn-show-more").show(),0==o?a.find(".js-feed-btn-show-all-posts").hide():a.find(".js-feed-btn-show-all-posts").show())}function t_feed_generatePosts(e,t,o,a,s){var d=$("#rec"+o),r=d.find(".js-feed-container"),i=d.find(".js-feed").attr("data-feed-grid-type"),p="";if(void 0===t||0!==t.length){if(void 0!==t)for(var n=0;n<t.length;n++){var _=t[n];"col"==i&&(p+=""+t_feed_drawSeparator(o,a)),p+=t_feed_drawWholePost(o,_,a,s),window.tFeedPosts[o].count=window.tFeedPosts[o].count+1,window.tFeedPosts[o][_.uid]=_}if(r.append(p),t_feed_initPopup(o,e,window.tFeedPosts,a),t_feed_unifyColHeights(o,a),"y"==window.lazy&&t_lazyload_update(),!a.previewmode)try{addEditFieldEvents_new(o)}catch(e){console.log(e)}}else d.find(".js-feed-container").append(t_feed_drawEmptyMessage(a,o))}function t_feed_drawSeparator(e,t){var o="";return 0<window.tFeedPosts[e].count&&window.tFeedPosts[e].count%t.blocksInRow==0&&(o+='<div class="t-clear t-feed__grid-separator" style="margin-bottom:'+t.vindent+'"></div>'),o}function t_feed_showMore(t,o){var e=$("#rec"+t),a=e.find(".js-feed-container"),s=(parseInt(a.attr("data-feed-show-count")),1);null!==e.find(".js-feed-btn-show-more")&&e.find(".js-feed-btn-show-more").on("click",function(e){e.preventDefault(),window.tFeedPosts[t].switcher?s=2:s++,window.tFeedPosts[t].switcher=!1,t_feed_loadPosts(t,o,s,!0)})}function t_feed_initFeedParts(t,o){var a=$("#rec"+t);a.find(".js-feed-parts-switcher").on("click",function(){var e=$(this).attr("data-feedpart-uid");a.find(".t-active").removeClass("t-active"),$(this).addClass("t-active"),o.feeduid=o.feeduid.split("-")[0]+"-"+e,window.tFeedPosts[t].switcher=!0,window.tFeedPosts[t].cou