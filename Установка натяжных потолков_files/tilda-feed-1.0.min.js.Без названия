function t_feed_init(o,a){window.tFeedPosts={},t_feed_createObjWithPosts();var e=$("#rec"+o),s=e.find(".js-feed"),t=$(".t-records").attr("data-tilda-mode");a.isPublishedPage="edit"!==t&&"preview"!==t,(a.btnText&&a.amountOfPosts||a.btnAllPosts.text)&&s.append(t_feed_drawBtnsContainer(a)),"y"==window.lazy&&t_lazyload_update(),setTimeout(function(){e.find(".js-feed-preloader")&&e.find(".js-feed-preloader").removeClass("t-feed__post-preloader__container_hidden")},1500),t_feed_showLoadersForPostsList(o),0<window.location.hash.length?t_feed_loadPosts(o,a,1,!1,t_feed_loadPartPosts):t_feed_loadPosts(o,a,1,!1),t_feed_showMore(o,a),"col"==s.attr("data-feed-grid-type")&&($(window).bind("resize",t_throttle(function(){t_feed_unifyColHeights(o,a)},200)),$(".t-feed").bind("displayChanged",function(){t_feed_unifyColHeights(o,a)})),window.onpopstate=function(){if(null!==window.history.state){var e=window.history.state.obj,t=window.history.state.post;t_feed_workPostPopup(o,e,t,t.uid,a,s)}else $(".t-body").hasClass("t-body_popupshowed")&&t_feed_closePopup(o,a,window.location.href)},window.tFeedPosts[o].title=$("title").text()}function t_feed_loadPartPosts(e,o,t,a){var s=$("#rec"+e);s.find(".js-feed-parts-switcher").each(function(){var e=decodeURI(window.location.hash).split("/");if($(this).attr("data-feedpart-title")==e[e.length-1]){var t=$(this).attr("data-feedpart-uid");o.feeduid=o.feeduid.split("-")[0]+"-"+t,s.find(".t-active").removeClass("t-active"),$(this).addClass("t-active")}}),window.tFeedPosts[e].switcher=!0,window.tFeedPosts[e].count=0,t_feed_loadPosts(e,o,t,a),$(window).load(function(){if(-1!=window.location.hash.indexOf("#!/tfeeds")){var e=decodeURI(window.location.hash).split("/"),t=e[e.length-1],o=$('.js-feed-parts-switcher[data-feedpart-title="'+t+'"]').parents(".js-feed").offset();t_feed_scrollToFeed(void 0!==o?o.top:0)}})}function t_feed_scrollToFeed(e){var t=void 0!==e&&void 0!==$(".js-feed").offset()?e-100:0;$("html, body").animate({scrollTop:t},10)}function t_feed